{
"manifest_version": 2,
"version": "1.0",
"name": "Grab the Logo",
"description": "Countdown Timer - The countdown timer is created which takes in a particular date and time and displays the time left for the particular date. ",
"browser_action":
{
"default_icon": "images/world.png",
"default_popup": "popup.html"
},
"content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["script.js"]
    }
  ],
  "options_page": "",
"icons":
{
"16": "images/world.png", 
"48": "images/world.png",
"128":"images/world.png"
},
"permissions": [
    "notifications","tabs", "<all_urls>"
  ]
}