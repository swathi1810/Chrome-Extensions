{
"manifest_version": 2,
"version": "1.0",
"name": "Source Code Extractor",
"description": "The source code extractor pulls up the source code of any HTML page that you are currently located at",
"browser_action":
{
"default_icon": "images/world.png",
"default_popup": "popup.html"
},
"content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["script.js"]
    }
  ],
  "options_page": "",
"icons":
{
"16": "images/world.png", 
"48": "images/world.png",
"128":"images/world.png"
},
"permissions": [
    "notifications","tabs", "<all_urls>"
  ]
}
